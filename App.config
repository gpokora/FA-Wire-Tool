<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="fireAlarmSettings" type="System.Configuration.AppSettingsSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>

  <appSettings>
    <add key="ConfigPath" value="config.json" />
    <add key="EnableLogging" value="true" />
    <add key="LogLevel" value="Info" />
    <add key="MaxLogFileSize" value="10485760" />
    <add key="EnableAutoUpdate" value="false" />
    <add key="UpdateUrl" value="https://firealarmsystems.com/updates" />
  </appSettings>

  <fireAlarmSettings>
    <add key="DefaultSystemVoltage" value="29.0" />
    <add key="DefaultMinVoltage" value="16.0" />
    <add key="DefaultMaxLoad" value="3.0" />
    <add key="DefaultReservedPercent" value="20" />
    <add key="DefaultWireGauge" value="16 AWG" />
    <add key="DefaultSupplyDistance" value="50.0" />
    <add key="NFPA72Compliance" value="true" />
  </fireAlarmSettings>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="textWriterTraceListener"
             type="System.Diagnostics.TextWriterTraceListener"
             initializeData="FireAlarmCircuit_trace.log" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>